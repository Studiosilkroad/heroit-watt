        </mj-column>

        <mj-column width="50%">
        <mj-image src="https://i.ibb.co/3p51Tsy/prize-2.png" width="300px" align="center"  padding-bottom="0px" padding-left="5px" padding-right="5px" padding-top="10px"   target="_blank"></mj-image>
         
        <mj-spacer height="20px" />
